<?xml version="1.0"?>
<sdf version="1.6">
  <model name="tadeocar_v1">
    <static>false</static>

    <!-- Base Link (Chasis Principal) -->
    <link name="base_link">
      <pose>0 0 0.1 0 0 0</pose>

      <inertial>
        <mass>50.0</mass>
        <inertia>
          <ixx>4.0</ixx>
          <iyy>4.0</iyy>
          <izz>2.0</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>

      <collision name="base_collision">
        <pose>0 0 0.155 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://tadeocar_v1/meshes/chassis/chasis_base.dae</uri>
            <scale>1 1 1</scale>
