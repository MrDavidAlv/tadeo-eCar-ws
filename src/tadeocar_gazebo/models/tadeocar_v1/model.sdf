        <inertia>
          <ixx>0.0001</ixx>
          <iyy>0.0001</iyy>
          <izz>0.0001</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>

      <visual name="camera_right_visual">
        <pose>0 0 0 0 1.5708 0</pose>
        <geometry>
          <cylinder>
            <radius>0.01</radius>
            <length>0.01</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1</ambient>
          <diffuse>0.1 0.1 0.1 1</diffuse>
        </material>
      </visual>

      <sensor name="camera_right_sensor" type="camera">
        <pose>0.005 0 0 0 0 0</pose>
        <always_on>true</always_on>
        <update_rate>30</update_rate>
        <visualize>true</visualize>
        <topic>camera/right/image_raw</topic>
        <gz_frame_id>camera_right_optical</gz_frame_id>
        <camera name="camera_right">
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>1280</width>
            <height>720</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>300</far>
          </clip>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </camera>
      </sensor>
    </link>

    <joint name="camera_right_joint" type="fixed">
      <parent>stereo_camera_base</parent>
      <child>camera_right</child>
    </joint>

    <!-- ==================== CAMERA OPTICAL FRAMES ==================== -->
    <link name="camera_left_optical">
      <pose>0.585 0.06 0.50 -1.5708 0 -1.5708</pose>
    </link>

    <joint name="camera_left_optical_joint" type="fixed">
      <parent>camera_left</parent>
      <child>camera_left_optical</child>
    </joint>

    <link name="camera_right_optical">
      <pose>0.585 -0.06 0.50 -1.5708 0 -1.5708</pose>
    </link>

    <joint name="camera_right_optical_joint" type="fixed">
      <parent>camera_right</parent>
      <child>camera_right_optical</child>
    </joint>

    <!-- ==================== GZ SIM PLUGINS ==================== -->

    <!-- Joint State Publisher (publishes all joint states) -->
    <plugin filename="ignition-gazebo-joint-state-publisher-system"
            name="ignition::gazebo::systems::JointStatePublisher">
      <topic>joint_states</topic>
      <joint_name>front_left_steering_joint</joint_name>
      <joint_name>front_left_wheel_joint</joint_name>
      <joint_name>front_right_steering_joint</joint_name>
      <joint_name>front_right_wheel_joint</joint_name>
      <joint_name>rear_left_steering_joint</joint_name>
      <joint_name>rear_left_wheel_joint</joint_name>
      <joint_name>rear_right_steering_joint</joint_name>
      <joint_name>rear_right_wheel_joint</joint_name>
    </plugin>

    <!-- Joint Controllers (velocity control for each joint) -->
    <!-- Front Left Steering -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>front_left_steering_joint</joint_name>
      <topic>model/tadeocar/joint/front_left_steering_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Front Left Wheel -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>front_left_wheel_joint</joint_name>
      <topic>model/tadeocar/joint/front_left_wheel_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Front Right Steering -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>front_right_steering_joint</joint_name>
      <topic>model/tadeocar/joint/front_right_steering_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Front Right Wheel -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>front_right_wheel_joint</joint_name>
      <topic>model/tadeocar/joint/front_right_wheel_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Rear Left Steering -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>rear_left_steering_joint</joint_name>
      <topic>model/tadeocar/joint/rear_left_steering_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Rear Left Wheel -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>rear_left_wheel_joint</joint_name>
      <topic>model/tadeocar/joint/rear_left_wheel_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Rear Right Steering -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>rear_right_steering_joint</joint_name>
      <topic>model/tadeocar/joint/rear_right_steering_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Rear Right Wheel -->
    <plugin filename="ignition-gazebo-joint-controller-system"
            name="ignition::gazebo::systems::JointController">
      <joint_name>rear_right_wheel_joint</joint_name>
      <topic>model/tadeocar/joint/rear_right_wheel_joint/cmd_vel</topic>
      <use_velocity_commands>true</use_velocity_commands>
    </plugin>

    <!-- Odometry Publisher (publishes /odom from model pose) -->
    <plugin filename="ignition-gazebo-odometry-publisher-system"
            name="ignition::gazebo::systems::OdometryPublisher">
      <odom_frame>odom</odom_frame>
      <robot_base_frame>base_link</robot_base_frame>
      <odom_topic>odom</odom_topic>
      <dimensions>3</dimensions>
    </plugin>

  </model>
</sdf>
